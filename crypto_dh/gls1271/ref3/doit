gcc -c -O2 mrcore.c
gcc -c -O2 mrarth0.c
gcc -c -O2 mrarth1.c
gcc -c -O2 mrio1.c
gcc -c -O2 mrbits.c
gcc -c -O2 mrarth2.c
gcc -c -O2 mrmonty.c
